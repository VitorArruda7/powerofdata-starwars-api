# API Keys (comma-separated, optional - if not set, no auth required)
API_KEYS=your_api_key_here,another_key

# Redis URL for caching (optional - if not set, uses in-memory cache)
REDIS_URL=redis://localhost:6379

# GCP Project ID (for deployment)
GCP_PROJECT_ID=your-project-id

# GCP Function Name
GCP_FUNCTION_NAME=starwars-function

# GCP Region
GCP_REGION=us-central1

# GCP Memory allocation (MB)
GCP_MEMORY=256

# GCP Timeout (seconds)
GCP_TIMEOUT=60
